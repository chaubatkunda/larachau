{
  "name": "larachau/ui",
  "description": "Minimal Laravel authentication scaffolding Inertia Vue Bootstrap starter kit",
  "type": "library",
  "license": "MIT",
  "version": "1.0.4",
  "keywords": [
    "larachau",
    "larachau/ui",
    "laravel",
    "inertia",
    "vue",
    "bootstrap",
    "starter-kit"
  ],
  "autoload": {
    "psr-4": {
      "Larachau\\Ui\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Chau Batkunda",
      "email": "maniklusi31@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Larachau\\Ui\\LarachauServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/console": "^11.0|^12.0",
    "illuminate/filesystem": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/validation": "^11.0|^12.0",
    "inertiajs/inertia-laravel": "^0.2.5"
  },
  "required-dev": {
    "laravel/framework": "^11.0|^12.0"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
